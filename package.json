{
  "name": "header_lmistie",
  "displayName": "LMISTIE Header",
  "description": "LMISTIE header for VSCode : www.le-101.fr",
  "version": "0.42.7",
  "publisher": "lmistie",
  "repository": {
    "type": "git",
    "url": "https://github.com/lmistie/Vscode-header.git/"
  },
  "homepage": "https://github.com/lmistie/Vscode-header.git",
  "license": "MIT",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "101header.username": {
          "type": "string",
          "description": "Username to use in headers"
        },
        "101header.email": {
          "type": "string",
          "description": "Email address to use in headers"
        }
      }
    },
    "commands": [
      {
        "command": "101header.insertHeader",
        "title": "Insert header",
        "when": "editorTextFocus"
      }
    ],
    "keybindings": [
      {
        "command": "101header.insertHeader",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^6.0.45",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "moment": "^2.15.0"
  }
}
